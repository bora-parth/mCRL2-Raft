[true*] forall id1, term1, commitIndex1: Nat, log1: LogType . val(#log1 > 0) => [exposeLog(id1, term1, commitIndex1, log1)] [true*] forall id2, term2, commitIndex2: Nat, log2: LogType, index: Nat .val(index < #log1 && index < #log2 && log1 . index == log2 . index && (#log2 > 0) && id1 != id2) => [exposeLog(id2, term2, commitIndex2, log2)] val(slice(log1, 0, index+1) == slice(log2, 0, index+1))